{
    "manifest_version": 3,
    "name": "Zedmonkey",
    "version": "1.0.0",
    "description": "A open-source userscript manager engineered for developers who demand speed and efficiency.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": [
      "storage",
      "scripting",
      "tabs",
      "contextMenus"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content/content.js"],
        "run_at": "document_start"
      }
    ],
    "options_ui": {
      "page": "options/options.html",
      "open_in_tab": true
    }
  }